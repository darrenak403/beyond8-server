{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AWS": {
    "S3": {
      "Region": "ap-southeast-1",
      "BucketName": "beyond8-storage",
      "CloudFrontUrl": "d30z0qh7rhzgt8.cloudfront.net"
    }
  },
  "JwtBearer": {
    "SecretKey": "YOUR_SECRET_KEY_HERE_MINIMUM_32_CHARACTERS_LONG",
    "Issuer": "https://your-domain.com",
    "Audience": "https://your-domain.com",
    "ExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 7,
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidateLifetime": true,
    "ValidateIssuerSigningKey": true,
    "ClockSkewSeconds": 0
  },
  "Gemini": {
    "ApiEndpoint": "https://generativelanguage.googleapis.com/v1beta",
    "DefaultModel": "gemini-2.5-flash-lite",
    "MaxRetries": 3,
    "TimeoutSeconds": 30,
    "RequestsPerMinute": 60,
    "RequestsPerDay": 1500,
    "MaxConcurrentRequests": 5,
    "DefaultParameters": {
      "MaxTokens": 8192,
      "Temperature": 0.7,
      "TopP": 0.95,
      "TopK": 40
    },
    "Pricing": {
      "InputCostPer1MTokens": 0.075,
      "OutputCostPer1MTokens": 0.30
    }
  },
  "Email": {
    "Resend": {
      "FromEmail": "noreply@beyond8.io.vn",
      "FromName": "Beyond8"
    }
  },
  "VnptEkyc": {
    "BaseUrl": "https://api.idg.vnpt.vn/",
    "TimeoutSeconds": 30
  }
}