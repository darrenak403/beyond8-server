{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AiProvider": "Bedrock",
  "AWS": {
    "S3": {
      "Region": "ap-southeast-1",
      "BucketName": "beyond8-storage",
      "CloudFrontUrl": "d30z0qh7rhzgt8.cloudfront.net"
    },
    "Bedrock": {
      "Region": "us-east-1",
      "DefaultModel": "anthropic.claude-3-5-sonnet-20240620-v1:0",
      "MaxRetries": 3,
      "TimeoutSeconds": 30,
      "RequestsPerMinute": 60,
      "RequestsPerDay": 1500,
      "MaxConcurrentRequests": 5,
      "DefaultParameters": {
        "MaxTokens": 8192,
        "Temperature": 0.7,
        "TopP": 0.95
      },
      "Pricing": {
        "InputCostPer1MTokens": 3.00,
        "OutputCostPer1MTokens": 15.00
      }
    }
  },
  "JwtBearer": {
    "SecretKey": "YOUR_SECRET_KEY_HERE_MINIMUM_32_CHARACTERS_LONG",
    "Issuer": "https://your-domain.com",
    "Audience": "https://your-domain.com",
    "ExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 7,
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidateLifetime": true,
    "ValidateIssuerSigningKey": true,
    "ClockSkewSeconds": 0
  },
  "Gemini": {
    "ApiEndpoint": "https://generativelanguage.googleapis.com/v1beta",
    "DefaultModel": "gemini-2.5-flash",
    "MaxRetries": 3,
    "TimeoutSeconds": 30,
    "RequestsPerMinute": 60,
    "RequestsPerDay": 1500,
    "MaxConcurrentRequests": 5,
    "DefaultParameters": {
      "MaxTokens": 8192,
      "Temperature": 0.7,
      "TopP": 0.95,
      "TopK": 40
    },
    "Pricing": {
      "InputCostPer1MTokens": 0.075,
      "OutputCostPer1MTokens": 0.30
    }
  },
  "Email": {
    "Resend": {
      "FromEmail": "noreply@beyond8.io.vn",
      "FromName": "Beyond8"
    }
  },
  "VnptEkyc": {
    "BaseUrl": "https://api.idg.vnpt.vn/",
    "TimeoutSeconds": 30
  },
  "Qdrant": {
    "Host": "localhost",
    "Port": 6333,
    "UseHttps": false,
    "ApiKey": null,
    "TimeoutSeconds": 30,
    "VectorDimension": 384
  },
  "HuggingFace": {
    "ApiEndpoint": "https://router.huggingface.co/hf-inference/models",
    "DefaultModel": "sentence-transformers/all-MiniLM-L6-v2/pipeline/feature-extraction",
    "MaxRetries": 3,
    "TimeoutSeconds": 30
  },
  "FrontendUrl": "https://beyond8.io.vn",
  "Clients": {
    "Identity": {
      "BaseUrl": "https://localhost:7123"
    }
  }
}